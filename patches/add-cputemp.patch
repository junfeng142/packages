+<%+header%>
+
+<script type="text/javascript" src="<%=resource%>/cbi.js?v=git-21.114.56541-974fb04"></script>
+<script type="text/javascript">//<![CDATA[
+	function progressbar(v, m)
+	{
+		var vn = parseInt(v) || 0;
+		var mn = parseInt(m) || 100;
+		var pc = Math.floor((100 / mn) * vn);
