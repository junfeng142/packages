+function index()
+	if not nixio.fs.access("/etc/config/aria2") then
+		return
+	end
+
+	entry({"admin", "nas", "aria2"}, cbi("aria2"), _("Aria2"), 220).dependent = true
+	entry({"admin", "nas", "aria2", "status"}, call("status")).leaf = true
