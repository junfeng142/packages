+function bt_tracker.cfgvalue(self, section)
+	local rv = {}
+	local val = Value.cfgvalue(self, section)
+	if type(val) == "table" then
+		val = table.concat(val, ",")
+	elseif not val then
+		val = ""
+	end
+	for v in val:gmatch("[^,%s]+") do
+		rv[#rv+1] = v
+	end
+	return rv
+end
