+
+CLANG_VER:=16-init-4609-g025a5b22-2
+CLANG_FILE:=clang-llvmorg-$(CLANG_VER).tgz
+define Download/CLANG
+	URL:=https://commondatastorage.googleapis.com/chromium-browser-clang/Linux_x64
+	URL_FILE:=$(CLANG_FILE)
