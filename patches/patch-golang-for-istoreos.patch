-# GOOS / GOARCH
-
-go_arch=$(subst \
-  aarch64,arm64,$(subst \
-  i386,386,$(subst \
-  mipsel,mipsle,$(subst \
-  mips64el,mips64le,$(subst \
-  powerpc64,ppc64,$(subst \
