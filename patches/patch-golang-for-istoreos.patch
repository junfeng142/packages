-	-extldflags '$(patsubst -z%,-Wl$(comma)-z$(comma)%,$(TARGET_LDFLAGS))'
-
-GO_PKG_CUSTOM_LDFLAGS= \
-	$(GO_PKG_LDFLAGS) \
-	$(patsubst %,-X %,$(GO_PKG_LDFLAGS_X))
-
-GO_PKG_INSTALL_ARGS= \
-	-v \
